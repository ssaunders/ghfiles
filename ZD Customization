   /* Function insertBOOpenerEl
      Inserts the notification square for when the user pushes the copy button*/
   function insertBOOpenerEl() {
      var elCss = `
            .bo-opener {
               height: 20px;
               width: 20px;
               padding-left: 3px;
               padding-top: 2px;

               display: flex;
               margin: 13px 20px;
               flex-shrink: 0;
